  object gbVideoSettings: TGroupBox
    Left = 12
    Top = 12
    Width = 273
    Height = 285
    Caption = ' Video Settings '
    TabOrder = 2
    object lbGameResolution: TLabel
      Left = 19
      Top = 32
      Width = 82
      Height = 14
      Hint = 
        'Set the in-game resolution. The game correctly supports all reso' +
        'lutions and aspect ratios (including custom and multi-monitor co' +
        'nfigurations) as long as the resolution is reported by Windows a' +
        'nd available in 8-bit color mode. It is recommended to select yo' +
        'ur native resolution.'
      AutoSize = False
      Caption = 'Game Resolution:'
      ParentShowHint = False
      ShowHint = True
      OnDblClick = lbGameResolutionDblClick
    end
    object lbSfxLimit: TLabel
      Left = 19
      Top = 245
      Width = 104
      Height = 13
      Hint = 
        'Set the limit for special effects such as smoke and wakes. The "' +
        'Original" setting is the original TA default and the "Enhanced" ' +
        'setting is the Unofficial Patch default.'
      AutoSize = False
      Caption = 'Special Effects Limit:'
      ParentShowHint = False
      ShowHint = True
    end
    object cbDisplayModes: TComboBox
      Left = 129
