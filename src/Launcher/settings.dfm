      Items.Strings = (
        'Original'
        'Enhanced')
    end
    object rgRenderingMode: TRadioGroup
      Left = 19
      Top = 65
      Width = 234
      Height = 73
      Hint = 
        'Select the fullscreen DirectDraw renderer or the windowed GDI re' +
        'nderer. Fullscreen mode is hardware accelerated and supports all' +
        ' patch features, but you may have to deal with various minor iss' +
        'ues with newer operating systems and GPU drivers such as flicker' +
        'ing caused by some background processes or corrupt graphics caus' +
        'ed by rotating desktop backgrounds. Windowed mode will always wo' +
        'rk without issues but is not hardware accelerated and does not s' +
        'upport many new patch features such as the megamap, whiteboard, ' +
        'line building, and multiplayer allied resource bars.'
      Caption = ' Rendering Mode: '
      Items.Strings = (
        'Fullscreen (DirectDraw)'
        'Windowed (GDI)')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 2
    end
    object rgMenuResolution: TRadioGroup
      Left = 19
      Top = 149
      Width = 234
      Height = 73
      Hint = 
        'Choose whether the main menu will be displayed at the original 6' +
        '40x480 resolution or be matched with the game resolution. Note t' +
        'hat setting the main menu to the game resolution will simply add' +
        ' black space around the menu and will not upscale the menu graph' +
        'ics. The main purpose of this setting is to allow the game to be' +
        ' visible on displays that no longer support the legacy 640x480 v' +
        'ideo mode.'
      Caption = ' Menu Resolution: '
      Items.Strings = (
        '640x480'
        'Game Resolution')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 3
    end
  end
  object gbAudioSettings: TGroupBox
    Left = 300
    Top = 12
    Width = 273
    Height = 193
    Caption = ' Audio Settings '
    TabOrder = 3
    object lbSoundsLimit: TLabel
      Left = 19
      Top = 125
      Width = 104
      Height = 13
      Hint = 
        'Set the maximum number of simultaneous sounds before sounds are ' +
        'cut off. The original TA default is "8" and the Unofficial Patch' +
        ' default is "Unlimited", which results in no sound cutoffs.'
      AutoSize = False
      Caption = 'Sounds Limit:'
      ParentShowHint = False
      ShowHint = True
    end
    object lbMusicMode: TLabel
      Left = 19
      Top = 158
      Width = 104
      Height = 13
      Hint = 
        'Set the music playback mode. "Play All" will play all music trac' +
        'ks in order, "Random" will play music tracks randomly, and "Repe' +
        'at" will repeat a music track. "Custom" is the original TA defau' +
        'lt and will attempt to utilize the dynamic music system in which' +
        ' the build music tracks play during moments of peace and the bat' +
        'tle music tracks play during moments of action. However, this se' +
        'tting is extremely buggy and no longer works on most modern mach' +
        'ines, so the Unofficial Patch default is "Random". If the dynami' +
        'c music system works on your machine, set this to "Custom".'
      AutoSize = False
      Caption = 'Music Mode:'
      ParentShowHint = False
      ShowHint = True
    end
    object cbSoundsLimit: TComboBox
      Left = 129
      Top = 122
      Width = 124
      Height = 21
      Style = csDropDownList
      ItemHeight = 13
      TabOrder = 0
      Items.Strings = (
        '8'
        '16'
        '32'
        'Unlimited')
    end
    object rgMusic: TRadioGroup
      Left = 151
      Top = 24
      Width = 102
      Height = 86
      Hint = 
        'Enable or disable the musical soundtrack. Note that if you chang' +
        'e this setting via the in-game Options menu, the game must be re' +
        'started before the change will take effect.'
      Caption = ' Music: '
      Items.Strings = (
        'Disabled'
        'Enabled')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 1
    end
    object rgSoundMode: TRadioGroup
      Left = 20
      Top = 24
      Width = 117
      Height = 85
      Hint = 
        'Set the sound to "Off", "Mono", or "3D Positional". When set to ' +
        '"3D Positional", the game'#39's positional sound data will be mapped' +
        ' to the speaker layout specified in the Windows Control Panel. A' +
        'll speaker layouts are supported (stereo, 5.1, 7.1, etc.). For e' +
        'xample, with a 5.1 speaker setup, sounds to the north of the cam' +
        'era will be mapped to the center speaker, sounds to the south of' +
        ' the camera will be mapped to the rear speakers, and sounds to t' +
        'he west or east of the camera will be mapped to the left or righ' +
        't speakers.'
      Caption = ' Sound Mode: '
      Items.Strings = (
        'Off'
        'Mono'
        '3D Positional')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 2
    end
