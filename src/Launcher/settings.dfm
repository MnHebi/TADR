    object lbPathfinding: TLabel
      Left = 19
      Top = 65
      Width = 104
      Height = 13
      Hint = 
        'Set the quality of the unit pathfinding. The lower the setting, ' +
        'the more quickly the pathfinding will degrade as more units are ' +
        'built. Higher settings require more CPU speed, but only when the' +
        're are many units in-game. The "Original (Very Low)" setting is ' +
        'the original TA default and is absolutely not recommended becaus' +
        'e even the "Low" setting provides a huge improvement in pathfind' +
        'ing quality.'
      AutoSize = False
      Caption = 'Pathfinding Quality:'
      ParentShowHint = False
      ShowHint = True
    end
