    object rgMouseMode: TRadioGroup
      Left = 19
      Top = 24
      Width = 234
      Height = 50
      Hint = 
        'Set the mouse interface mode to left-click or right-click. In th' +
        'e left-click mode, which is the TA default, units are both selec' +
        'ted and ordered with the left mouse button and deselected with t' +
        'he right mouse button. In the right-click mode, which is the def' +
        'ault in most other RTS games, units are selected and deselected ' +
        'with the left mouse button and ordered with the right mouse butt' +
        'on.'
      Caption = ' Mouse Mode: '
      Columns = 2
      Items.Strings = (
        'Left-Click'
        'Right-Click')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 0
    end
    object rgDblClickSel: TRadioGroup
      Left = 19
      Top = 80
      Width = 234
      Height = 50
      Hint = 
        'Enable or disable the new "double-click to select all units of t' +
        'he same type on screen" feature.'
      Caption = ' Double-Click Selection: '
      Columns = 2
      Items.Strings = (
        'Disabled'
        'Enabled')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 1
    end
    object rgGroupSelMod: TRadioGroup
      Left = 19
      Top = 136
      Width = 234
      Height = 50
      Hint = 
        'Enable or disable the group selection hotkey modifier. When enab' +
        'led, unit groups (set with CTRL + the number keys) will be selec' +
        'ted with the number keys and build menus will be selected with A' +
        'LT + the number keys. When disabled, unit groups will be selecte' +
        'd with ALT + the number keys and build menus will be selected wi' +
        'th the number keys. The original TA default is "Disabled" and th' +
        'e Unofficial Patch default is "Enabled".'
      Caption = ' Group Selection Hotkey Modifier: '
      Columns = 2
      Items.Strings = (
        'Disabled'
        'Enabled')
      ParentShowHint = False
      ShowHint = True
      TabOrder = 2
    end
